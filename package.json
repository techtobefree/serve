{
  "name": "serve2free",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "npx cap run android",
    "dev:ios": "npx cap run ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "sync": "pnpm build && npx cap sync",
    "build": "tsc -b && vite build",
    "build:android": "npx cap build android",
    "build:ios": "npx cap build ios",
    "build:all": "pnpm sync && pnpm build:android && pnpm build:ios",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "db:new": "npx supabase migration new",
    "db:start": "npx supabase start",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset && pnpm db:types",
    "db:types": "npx supabase gen types typescript --schema public,gis > src/domains/db/supabaseTypes.ts --local",
    "icon:gen": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^6.0.1",
    "@capacitor/camera": "^6.1.1",
    "@capacitor/core": "^6.1.2",
    "@capacitor/device": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@date-fns/tz": "^1.2.0",
    "@generouted/react-router": "^1.19.8",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/react": "^8.3.4",
    "@ngneat/falso": "^7.2.0",
    "@supabase/supabase-js": "^2.46.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-form": "^0.34.4",
    "@tanstack/react-query": "^5.59.20",
    "@vis.gl/react-google-maps": "^1.4.0",
    "date-fns": "^4.1.0",
    "ionicons": "^7.4.0",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.1.2",
    "@eslint/js": "^9.13.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.13.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.20.5"
  }
}