{
  "name": "serve2free",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "npx cap run android",
    "dev:ios": "npx cap run ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "sync": "pnpm build && npx cap sync",
    "build": "tsc -b && vite build",
    "build:android": "npx cap build android",
    "build:ios": "npx cap build ios",
    "build:all": "pnpm sync && pnpm build:android && pnpm build:ios",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:pwa": "pwa-assets-generator --preset minimal-2023 public/logo.svg"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@generouted/react-router": "^1.19.8",
    "@ionic/react": "^8.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.20.5"
  }
}