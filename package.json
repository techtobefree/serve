{
  "name": "serve2free",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "npx cap run android",
    "dev:ios": "npx cap run ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "sync": "pnpm build && npx cap sync",
    "build": "tsc -b && vite build",
    "build:android": "npx cap build android",
    "build:ios": "npx cap build ios",
    "build:all": "pnpm sync && pnpm build:android && pnpm build:ios",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "db:new": "npx supabase migration new",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:restart": "npx supabase stop && npx supabase start",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset && pnpm db:types",
    "db:types": "npx supabase gen types typescript --schema public,gis > supabase/functions/_shared/supabaseTypes.ts --local",
    "func:new": "npx supabase functions new",
    "func:debug": "npx supabase functions logs",
    "func:deploy": "npx supabase functions deploy",
    "func:serve": "npx supabase functions serve",
    "icon:gen": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "dependencies": {
    "@awesome-cordova-plugins/calendar": "^6.14.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/core": "^7.0.1",
    "@capacitor/device": "^7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@date-fns/tz": "^1.2.0",
    "@generouted/react-router": "^1.20.0",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/react": "^8.4.3",
    "@ngneat/falso": "^7.3.0",
    "@supabase/functions-js": "^2.4.4",
    "@supabase/supabase-js": "2.48.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-form": "^0.42.0",
    "@tanstack/react-query": "^5.66.0",
    "@vis.gl/react-google-maps": "^1.5.1",
    "date-fns": "^4.1.0",
    "ionicons": "^7.4.0",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.1",
    "@eslint/js": "^9.20.0",
    "@tailwindcss/vite": "^4.0.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/parser": "^8.24.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-import-resolver-typescript": "^3.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.2",
    "tailwindcss": "^4.0.6",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-pwa": "^0.21.1"
  }
}